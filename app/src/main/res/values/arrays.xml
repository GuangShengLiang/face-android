<?xml version="1.0" encoding="utf-8"?>
<resources>
    <integer-array name="google_colors">
        <item>@color/google_red</item>
        <item>@color/google_blue</item>
        <item>@color/google_yellow</item>
        <item>@color/google_green</item>
    </integer-array>
    <declare-styleable name="CustomTitleBar">
        <attr name="title" format="string"/>
        <attr name="left_icon" format="reference"/>
        <attr name="right_icon" format="reference"/>
        <attr name="right_text" format="string"/>
        <attr name="titlebar_type" format="integer"/>
    </declare-styleable>

</resources>